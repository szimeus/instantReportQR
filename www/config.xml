<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0"
	id="instant.report.qr" version="1.0.0">
	<name>instant Report QR</name>

	<description>
		A sample Apache Cordova application that responds to the
		deviceready
		event.
	</description>

	<author href="http://www.eclipse.org/thym" email="thym-dev@eclipse.org">
		Eclipse.org -
		Thym
	</author>

	<content src="index.html" />

	<access origin="*" />
	<preference name="fullscreen" value="true" />
	<preference name="webviewbounce" value="true" />

	<feature name="App">
		<param name="android-package" value="org.apache.cordova.App" />
	</feature>
	<feature name="BarcodeScanner">
		<param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
		<param name="ios-package" value="CDVBarcodeScanner" />
		<param name="wp-package" value="BarcodeScanner" />
		<param name="id" value="com.phonegap.plugins.barcodescanner" />
	</feature>
	<feature name="SplashScreen">
		<param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
		<param name="onload" value="true" />
		<param name="ios-package" value="CDVSplashScreen" />
		<param name="wp-package" value="SplashScreen" />
	</feature>
	<feature name="Splashscreen">
		<param name="id" value="org.apache.cordova.splashscreen" />
	</feature>

</widget>
